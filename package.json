{
  "name": "runninghub-auto-run2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:build-server": "tsc -p tsconfig.server.json",
    "dev:start": "node dist/server.js",
    "dev": "pnpm dev:build-server && pnpm dev:start",
    "dev:remote": "cross-env VITE_REMOTE_MODE=true pnpm dev",
    "build": "tsc -b && vite build",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build:prod": "cross-env NODE_ENV=production pnpm build",
    "preview": "vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@tailwindcss/vite": "^4.1.4",
    "antd": "^5.24.8",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwindcss": "^4.1.4",
    "uuid": "^9.0.1",
    "vite-plugin-ssr": "^0.4.142",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
